{
    "compilerOptions": {
        "target": "es5",
        "lib": ["es5", "dom"],
        "types": ["cypress", "node"],
        "resolveJsonModule": true, 
        "baseUrl": "../",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports":true,
        "paths": {
          "@fixtures/*":[
            "cypress/fixtures/*"
          ]
        },
      },
      "include": ["**/*.ts"]
}